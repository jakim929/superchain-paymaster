{
  "name": "@dapp-starter-with-backend/backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsup",
    "dev:local:services": "docker-compose up -d",
    "dev:service": "tsx src/cmd/runService.ts",
    "dev:worker": "tsx src/cmd/runWorker.ts",
    "dev": "pnpm dev:local:services && concurrently -n service,worker \"pnpm run dev:service\" \"pnpm run dev:worker\"",
    "start:service": "node dist/runService.js",
    "start:worker": "node dist/runWorker.js",
    "drizzle:generate": "drizzle-kit generate:pg"
  },
  "devDependencies": {
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.6.2",
    "@types/pg": "^8.10.7",
    "drizzle-kit": "^0.19.13",
    "tsup": "^7.2.0",
    "tsx": "^3.13.0"
  },
  "dependencies": {
    "@bull-board/api": "^5.9.1",
    "@bull-board/fastify": "^5.9.1",
    "@fastify/cors": "^8.4.1",
    "abitype": "^0.8.11",
    "bullmq": "^4.12.4",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "fastify": "^4.24.2",
    "luxon": "^3.4.3",
    "pg": "^8.11.3",
    "viem": "^1.18.8",
    "zod": "^3.22.4"
  }
}
